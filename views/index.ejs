        if (num < 100) return tens[Math.floor(num / 10)] + (num % 10 ? " " + belowTwenty[num % 10] : "");
        if (num < 1000) return belowTwenty[Math.floor(num / 100)] + " Hundred" + (num % 100 ? " " + numberToWords(num % 100) : "");
        return "";
    }

    function yearToWords(year) {
        if (year < 2000) {
            return numberToWords(year);
        } else if (year < 2010) {
            return "Two Thousand" + (year % 2000 !== 0 ? " " + numberToWords(year % 2000) : "");
        } else if (year < 2100) {
            return "Two Thousand " + numberToWords(year % 100);
        } else {
            return numberToWords(Math.floor(year / 100)) + " Hundred" + (year % 100 ? " " + numberToWords(year % 100) : "");
        }
    }

    let date = new Date(dateString);
    if (isNaN(date)) return "Invalid date";

    let day = date.getDate();
    let month = months[date.getMonth()];
    let year = date.getFullYear();

    let suffix = suffixes[day] || "th";

    return `${numberToWords(day)} of ${month} ${yearToWords(year)}`;
}


